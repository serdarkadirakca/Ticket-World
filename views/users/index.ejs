                    <h2 class="nav-link" style="color:#1f1e18; text-align: center;"><sup>Featured Concert Tickets</sup></h2>
                    <div class="container-fluid">
                        <div class="tab-content tab-content-carousel">
                            <div class="tab-pane p-0 fade show active" id="products-featured-tab" 
                                role="tabpanel" aria-labelledby="products-featured-link">
                                <div class="owl-carousel owl-simple carousel-equal-height carousel-with-shadow" data-toggle="owl" 
                                    data-owl-options='{
                                        "nav": false, 
                                        "dots": true,
                                        "margin": 20,
                                        "loop": false,
                                        "responsive": {
                                            "0": {
                                                "items":2
                                            },
                                            "480": {
                                                "items":2
                                            },
                                            "768": {
                                                "items":3
                                            },
                                            "992": {
                                                "items":4
                                            },
                                            "1200": {
                                                "items":5
                                            },
                                            "1600": {
                                                "items":6,
                                                "nav": true
                                            }
                                        }
                                    }'>
                                <% productCategory1.forEach(products => { %>
                                    <div class="product product-11 text-center">
                                        <figure class="product-media">
                                            <a href="/<%= products.url %>" name="product<%= products.id %>-url">
                                                <img src="static/images/products/<%= products.image %>" alt="Product image" class="product-image"width="600">
                                                <img src="static/images/products/<%= products.imagehover %>" alt="Product image" class="product-image-hover"width="600">
                                            </a>
                                            <!-- End .product-action-vertical -->
                                        </figure><!-- End .product-media -->
                                        
                                        <div class="product-body">
                                            <h3 class="product-title"><a href="/<%= products.url %>">
                                                <%= products.title %></a></h3>
                                            <div class="product-price">
                                                <p><sup><%= products.place %></sup></p>
                                            </div><!-- End .product-price -->
                                        </div><!-- End .product-body -->
                                        <div class="product-action">
                                            <a href="#preview-modal-<%= products.id %>" data-toggle="modal" class="btn-product"><span>quick view</span></a>
                                        </div><!-- End .product-action -->
                                    </div>
